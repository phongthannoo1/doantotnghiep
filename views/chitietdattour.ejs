				    if(ss<10){ss='0'+ss;}
				    var mimi = today.getMinutes();
				    if(mimi<10){mimi='0'+mimi;}
				    var hh = today.getHours();
				    if(hh<10){hh='0'+hh;}
				    var dd = today.getDate();
				    if(dd<10){dd='0'+dd;}
				    var mm = today.getMonth()+1; //January is 0!
				    if(mm<10){mm='0'+mm;}
				    var yyyy = today.getFullYear();
				    
				    today = yyyy+''+mm+''+dd+hh+mimi+ss;
				    
				    $("#mahoadon").val(today);				    
				});
			</script>
