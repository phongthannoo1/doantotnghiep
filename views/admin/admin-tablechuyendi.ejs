<!-- Breadcrumbs-->
      <ol class="breadcrumb">
        <li class="breadcrumb-item">
          <a href="#">Dashboard</a>
        </li>
        <li class="breadcrumb-item active">Chuyến đi</li>
      </ol>
      
      <!-- Example DataTables Card-->
      <div class="card mb-3">
        <div class="card-header">
          <i class="fa fa-table"></i> Danh sách Chuyến đi</div>
        <div class="card-body">
          <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
              <thead>
                <tr>
                  <th>ID Chuyến</th>
                  <th>Mã tour</th>
                  <th>Ngày xuất phát</th>
                  <th>Sô lượng</th>
                  <th>Đã đặt</th>
                </tr>
              </thead>
              <tfoot>
                <tr>
                  <th>ID Chuyến</th>
                  <th>Mã tour</th>
                  <th>Ngày xuất phát</th>
                  <th>Sô lượng</th>
                  <th>Đã đặt</th>
                </tr>
              </tfoot>
              <tbody>
                <%if(typeof nn !=="undefined"){%>
                <%nn.forEach(function(body){%>
                <tr>
                  <td><%=body.idchuyen%></td>
                  <td><%=body.matour%></td>
                  <td><%=body.ngayxp%></td>
                  <td><%=body.soluong%></td>
                  <td><%=body.dadat%></td>
                </tr>
                <%});}%>
              </tbody>
            </table>
          </div>
        </div>
        <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div>
      </div>
